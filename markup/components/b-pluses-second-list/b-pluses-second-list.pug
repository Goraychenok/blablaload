mixin b-pluses-second-list(data)
    +b.b-pluses-second-list
        .container
            +e.title Как мы работаем?
            +e.row
                for item in data.pluses
                    +e.item
                        +b-pluses-item({
                            img: item.img,
                            text: item.description
                        })
